<%- include('header') %>
        <link href="https://cdn.rawgit.com/TeaMeow/TocasUI/2.3.2/dist/tocas.css" rel='stylesheet'>
        <script src="https://cdn.rawgit.com/TeaMeow/TocasUI/2.3.2/dist/tocas.js"></script>
        <style type="text/css">
            .pusher {
                background-color: rgb(250, 250, 250);
            }
        </style>
        <!-- 左側邊欄 -->
        <div class="ts left vertical fluid inverted visible menu sidebar">

            <!-- 個人資料項目 -->
            <div class="center aligned item">
                <a href="/manager/manage">
                    <img class="ts tiny circular image" src="/img/school.png">
                    <br>
                    <br>
                    <div>School</div>
                </a>
            </div>
            <!-- / 個人資料項目 -->

            <!-- 使用者管理 -->
            <div class="item">
                <i class="users icon"></i>
                學生管理
                <div class="menu">
                    <a class="item" href="/manager/verifyApplication">審查申請資格</a>
                    <a class="item" href="/manager/querySuccessApplyStatusFalse">審查結果/發放餐券</a>
                </div>
            </div>
            <!-- / 使用者管理 -->

            <!-- 網站管理 -->
            <div class="item">
                <i class="setting icon"></i>
                公告管理
                <div class="menu">
                    <a class="item" href="/posts/create">新增貼文</a>
                </div>
            </div>
            <!-- / 網站管理 -->

            <!-- 餐券管理 -->
            <div class="item">
                <i class="setting icon"></i>
                餐券管理
                <div class="menu">
                    <a class="item" href="/manager/querySuccessApplyStatusFalse">審查結果/發放餐券</a>
                    <a class="item" href="/manager/queryUsedTicket">餐券使用狀況</a>
                    <!-- <a class="item" href="/manager/querySomeoneTicket">查詢個人餐券使用狀況</a> -->
                </div>
            </div>
            <!-- / 餐券管理 -->

            <!-- 管理文章 -->
            <a href="#!" class="item">
                <i class="newspaper icon"></i>
                管理文章
            </a>
            <!-- / 管理文章 -->

            <!-- 回報處理 -->
            <a href="#!" class="item">
                <i class="flag icon"></i>
                回報處理
            </a>
            <!-- / 回報處理 -->

            <!-- 底部選單 -->
            <div class="bottom menu">
                <!-- 登出 -->
                <a href="/signout" class="item">
                    <i class="sign out icon"></i>
                    登出
                </a>
                <!-- / 登出 -->
            </div>
            <!-- / 底部選單 -->
        </div>
        <!-- / 左側邊欄 -->

        <!-- 可擠壓式的推動容器 -->
        <div class="squeezable pusher">
            <%- include('../components/nav') %>
                <div class="ts fluid divided statistics">
                    <div class="statistic">
                        <div class="value">
                            <%=ticketLists.length %>
                        </div>
                        <div class="label">張餐券未使用</div>
                    </div>
                </div>
                <div id="first" class="ts top attached tabbed menu">
                    <a class="item" data-tab="已使用" href="/manager/queryUsedTicket">已使用</a>
                    <a class="active item" data-tab="未使用" href="/manager/queryNotUsedTicket">未使用</a>
                </div>
                <!-- 清單欄位 -->
                <div class="sixteen wide column">
                    <br>
                    <form method="post">
                        <table class="ts very basic striped single line table">
                            <thead>
                                <tr>
                                    <th>編號</th>
                                    <th>擁有者</th>
                                    <th>是否使用</th>
                                    <th>發行日期</th>
                                    <th>有效日期</th>
                                </tr>
                            </thead>
                            <tbody>
                                <%ticketLists.forEach(function(ticketList) { %>
                                    <tr>
                                        <td>
                                            <%=ticketList.ticketId%>
                                        </td>
                                        <td>
                                            <%=ticketList.owner%>
                                        </td>
                                        <td>
                                            <%=ticketList.isUsed%>
                                        </td>
                                        <td>
                                            <%=ticketList.issuedDate%>
                                        </td>
                                        <td>
                                            <%=ticketList.expDate%>
                                        </td>
                                    </tr>
                                    <% }) %>
                            </tbody>
                        </table>
                        <!-- / 清單欄位 -->
                    </form>
                </div>

        </div>
        <!-- / 可擠壓式的推動容器 -->
        <script>
            ts('#first.tabbed.menu .item').tab();
        </script>

        <%- include('footer') %>